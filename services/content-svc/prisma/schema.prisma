generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Material {
  id          String   @id @default(uuid())
  title       String
  type        String
  url         String
  description String?
  tags        String   
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
